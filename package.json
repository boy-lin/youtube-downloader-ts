{
  "name": "youtube-downloader-ts",
  "version": "1.0.0",
  "description": "A TypeScript library for downloading YouTube videos with stream resolution and download options",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "publish": "./publish.sh"
  },
  "bin": {
    "yt-dl-ts": "./dist/index.js"
  },
  "keywords": [
    "youtube",
    "download",
    "video",
    "audio",
    "stream",
    "typescript",
    "nodejs",
    "cli",
    "ffmpeg",
    "youtube-downloader"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/youtube-downloader-ts.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/youtube-downloader-ts/issues"
  },
  "homepage": "https://github.com/your-username/youtube-downloader-ts#readme",
  "dependencies": {
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
